package com;

public class InnerClassType2 {
	
	private String msg="Inner Classes";

	 void display(){  
		 class Inner{  
			 void msg(){
				 System.out.println(msg);
			 }  
	  }  
	  
	  Inner l=new Inner();  
	  l.msg();  
	 }  


	public static void main(String[] args) {
		
		InnerClassType2  ob=new InnerClassType2 ();  
		ob.display();  

	}

}